CREATE TABLE `blockchain`.`news` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `news_id` VARCHAR(45) NOT NULL,
  `time` VARCHAR(30) NULL,
  `source` VARCHAR(50) NULL,
  `tag` TEXT NULL,
  `title` VARCHAR(100) NULL,
  `detail` TEXT NULL,
  PRIMARY KEY (`id`),
  UNIQUE INDEX `newscol_UNIQUE` (`news_id` ASC));
